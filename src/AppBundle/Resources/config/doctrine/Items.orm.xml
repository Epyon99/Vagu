<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Items" table="items">
    <id name="iditem" type="integer" column="IdItem">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="codigo" type="string" column="Codigo" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="nombre" type="string" column="Nombre" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="precio" type="string" column="Precio" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="series" type="string" column="Series" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="idmarca" type="integer" column="IdMarca" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="descripcion" type="integer" column="Descripcion" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
